{
  "$schema": "https://deno.land/x/denon@2.5.0/schema.json",
  "scripts": {
    "start": {
      "cmd": "deno run server.ts",
      "desc": "run my server.ts file",
      "allow": ["read", "env", "net"],
      "env": {
        "ENV_NAME": "dev",
        "HOST": "localhost",
        "PORT": "4000",
        "EXPECTED_ORIGIN": "http://localhost:8000",
        "RELYING_PARTY_ID": "localhost",
        "RELYING_PARTY_NAME": "Graf",
        "GENERIC_TOKEN_EXPIRES_IN": "300000",
        "REGISTRATION_TOKEN_EXPIRES_IN": "30000",
        "AUTHENTICATION_TOKEN_EXPIRES_IN": "1y"
      },
      "unstable": true
    }
  }
}
